# Build caffe:
$ source setup.sh
$ make DEBUG=1 -j20
$ build/tools/caffe train --solver=examples/cifar10/cifar10_quick_solver.prototxt  --gpu 0,1


### CAFFE directed tests: 
# Run all tests:
./build/test/test_all.testbin

# Run a specific test:
./build/test/test_all.testbin --gtest_filter=Im2colKernelTest/0.TestND



